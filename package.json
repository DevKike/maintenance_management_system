  "main": "server.js",
  "scripts": {
    "dev": "ts-node-dev src/server.ts",
    "typeorm": "typeorm-ts-node-commonjs",
    "migration:run": "npm run typeorm migration:run -- -d ./src/infrastructure/database/config/typeorm.ts",
